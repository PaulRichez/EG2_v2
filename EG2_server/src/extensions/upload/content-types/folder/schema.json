{
  "kind": "collectionType",
  "collectionName": "upload_folders",
  "info": {
    "singularName": "folder",
    "pluralName": "folders",
    "displayName": "Folder",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "min": 1,
      "required": true,
      "configurable": false
    },
    "pathId": {
      "type": "integer",
      "unique": true,
      "required": true,
      "configurable": false
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::upload.folder",
      "inversedBy": "children",
      "configurable": false
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::upload.folder",
      "mappedBy": "parent",
      "configurable": false
    },
    "files": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::upload.file",
      "mappedBy": "folder",
      "configurable": false
    },
    "path": {
      "type": "string",
      "min": 1,
      "required": true,
      "configurable": false
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
